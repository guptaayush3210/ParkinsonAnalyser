<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.parkinsonanalyser"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.application.parkinsonanalyser.ParkinsonApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.application.parkinsonanalyser.Base" >
        </activity>
        <activity android:name="com.application.parkinsonanalyser.Home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.application.parkinsonanalyser.TapCount" >
        </activity>
        <activity android:name="com.application.parkinsonanalyser.TremorDetect" >
        </activity>
        <activity android:name="com.application.parkinsonanalyser.ReactionTest" >
        </activity>
        <activity android:name="com.application.parkinsonanalyser.About" >
        </activity>
        <activity android:name="com.application.parkinsonanalyser.SessionResult" >
        </activity>
    </application>

</manifest>