<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.parkinsonanalyser" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".ParkinsonApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".Base" >
        </activity>
        <activity android:name=".Home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TapCount" >
        </activity>
        <activity android:name=".TremorDetect" >
        </activity>
        <activity android:name=".ReactionTest" >
        </activity>
        <activity android:name=".About" >
        </activity>
        <activity android:name=".SessionResult" >
        </activity>
    </application>

</manifest>
